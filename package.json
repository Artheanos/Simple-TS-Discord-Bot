{
  "name": "my_bot_js",
  "version": "1.0.0",
  "description": "Simple discord bot",
  "main": "dist/index.js",
  "scripts": {
    "build": "node_modules/.bin/tsc",
    "build-start": "yarn run build && yarn run start",
    "lint": "eslint src/**/*.[tj]s",
    "start": "node dist/src/app.js",
    "types": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Artheanos/Simple-JS-Discord-Bot.git"
  },
  "author": "Jan Pieczul",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Artheanos/Simple-JS-Discord-Bot/issues"
  },
  "homepage": "https://github.com/Artheanos/Simple-JS-Discord-Bot#readme",
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.16.1",
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.2",
    "bent": "^7.3.12",
    "discord.js": "^14.14.1",
    "eslint": "^8.56.0",
    "eslint-plugin-jest": "^27.6.0",
    "libsodium-wrappers": "^0.7.13",
    "module-alias": "^2.2.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "jest": "^29.7.0",
    "prisma": "^5.7.1",
    "typescript": "^5.3.3",
    "yarn-upgrade-all": "^0.7.2"
  },
  "_moduleAliases": {
    "config": "dist/config"
  },
  "_moduleDirectories": [
    "dist/src"
  ],
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "config"
    ]
  },
  "eslintIgnore": [
    "node_modules"
  ]
}
