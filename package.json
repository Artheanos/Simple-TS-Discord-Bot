{
  "name": "my_bot_js",
  "version": "1.0.0",
  "description": "Simple discord bot",
  "main": "dist/index.js",
  "scripts": {
    "build": "node_modules/.bin/tsc",
    "build-start": "yarn run build && yarn run start",
    "lint": "eslint src/**/*.[tj]s",
    "start": "node dist/src/app.js",
    "types": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Artheanos/Simple-JS-Discord-Bot.git"
  },
  "author": "Jan Pieczul",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Artheanos/Simple-JS-Discord-Bot/issues"
  },
  "homepage": "https://github.com/Artheanos/Simple-JS-Discord-Bot#readme",
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.7.5",
    "@prisma/client": "4.8.0",
    "axios": "^0.24.0",
    "bent": "^7.3.12",
    "discord.js": "^13.5.1",
    "eslint": "^8.30.0",
    "eslint-plugin-jest": "^27.1.7",
    "libsodium-wrappers": "^0.7.9",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^18.11.18",
    "@types/ws": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "jest": "^27.4.7",
    "prisma": "^4.8.0",
    "ts-jest": "^27.1.2",
    "typescript": "^4.9.4",
    "yarn-upgrade-all": "^0.6.1"
  },
  "_moduleAliases": {
    "config": "dist/config"
  },
  "_moduleDirectories": [
    "dist/src"
  ],
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "config"
    ]
  },
  "eslintIgnore": [
    "node_modules"
  ]
}
